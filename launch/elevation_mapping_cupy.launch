<launch>
  <!-- Elevation mapping node -->
  <node pkg="elevation_mapping_cupy" type="elevation_mapping_node" name="elevation_mapping" output="screen">
    <rosparam command="load" file="$(find elevation_mapping_cupy)/config/parameters.yaml" />
  </node>

  <node pkg="elevation_mapping_cupy" type="elevation_mapping_node" name="elevation_mapping_large" output="screen">
    <rosparam command="load" file="$(find elevation_mapping_cupy)/config/map2_parameter.yaml" />
  </node>
</launch>
